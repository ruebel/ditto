    "node"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "angular": "*",
    "angular-animate": "*",
    "angular-aria": "*",
    "angular-messages": "~1.4.3",
    "angular-jwt": "*",
    "angular-material": "*",
    "angular-sanitize": "*",
    "angular-socket-io": "*",
    "angular-ui-router": "*",
    "a0-angular-storage": "*",
    "font-awesome": "*",
    "jquery": "*",
    "material-design-icons": "*",
    "moment": "*",
    "socket.io-client": "*",
    "toastr": "*"
  },
  "devDependencies": {
    "angular-mocks": "*",
    "sinon": "*",
    "bardjs": "~0.1.4"
  },
  "overrides": {
    "socket.io-client": {
      "main": "socket.io.js"
